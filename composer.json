{
    "name": "Search API",
    "description": "The Search API",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1",
        "bernard/bernard": "^0.12.3",
        "jms/serializer-bundle": "^2.0",
        "nelmio/solarium-bundle": "^2.4",
        "php-http/guzzle6-adapter": "^1.1",
        "ramsey/uuid": "^3.6",
        "sensio/framework-extra-bundle": "^3.0",
        "swagger-api/swagger-ui": "^3.1",
        "symfony/annotations-pack": "^1.0",
        "symfony/asset": "^3.3",
        "symfony/console": "^3.3",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^3.3",
        "symfony/security-bundle": "^3.3",
        "symfony/translation": "^3.3",
        "symfony/twig-bundle": "^3.3",
        "symfony/validator": "^3.3",
        "symfony/web-profiler-bundle": "^3.3",
        "symfony/yaml": "^3.3",
        "zircote/swagger-php": "^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpstan/phpstan": "^0.8.0",
        "phpunit/phpunit": "^6.0",
        "symfony/dotenv": "^3.3"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "make cache-warmup": "script",
            "assets:install --symlink --relative": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "extra": {
        "symfony": {
            "id": "01BKVTE0GGJ9MESG49ECBW3DB5",
            "allow-contrib": false
        }
    }
}
