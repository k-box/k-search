{
    "name": "k-link/k-search",
    "description": "The K-Link Search API",
    "type": "project",
    "license": "proprietary",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://git.klink.asia/main/k-registry-client.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/thePanz/query-translator.git"
        }
    ],
    "require": {
        "php": "^7.1",
        "bernard/bernard": "^0.12.3",
        "jms/serializer-bundle": "^2.0",
        "nelmio/solarium-bundle": "^3.0.0-beta",
        "netgen/query-translator": "dev-add-ranges-support",
        "oneofftech/k-link-registry-client": "^0.1",
        "php-http/cache-plugin": "^1.4",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/httplug-bundle": "^1.8",
        "ramsey/uuid": "^3.6",
        "sensio/framework-extra-bundle": "^5.0",
        "swagger-api/swagger-ui": "^3.2",
        "symfony/asset": "^4.0",
        "symfony/cache": "^4.0",
        "symfony/console": "^4.0",
        "symfony/dotenv": "^4.0",
        "symfony/event-dispatcher": "4.0.11 as 3.4.4",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/monolog-bundle": "^3.1",
        "symfony/proxy-manager-bridge": "^4.0",
        "symfony/security-bundle": "^4.0",
        "symfony/translation": "^4.0",
        "symfony/twig-bundle": "^4.0",
        "symfony/validator": "^4.0",
        "symfony/web-profiler-bundle": "^4.0",
        "symfony/yaml": "^4.0",
        "zircote/swagger-php": "^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "leofeyer/optimize-native-functions-fixer": "^1.1",
        "php-http/mock-client": "^1.0",
        "phpstan/phpstan-shim": "^0.9.2",
        "phpunit/phpunit": "^7.0",
        "sebastian/comparator": "^3.0",
        "symfony/browser-kit": "^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "make cache-warmup": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "extra": {
        "symfony": {
            "id": "01BKVTE0GGJ9MESG49ECBW3DB5",
            "allow-contrib": "true"
        }
    }
}
