{
    "name": "k-link/k-search",
    "description": "The K-Link Search API",
    "type": "project",
    "license": "proprietary",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/thePanz/query-translator.git"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-mbstring": "*",
        "doctrine/doctrine-migrations-bundle": "^1.3",
        "doctrine/orm": "^2.6",
        "jms/serializer-bundle": "^2.0",
        "nelmio/solarium-bundle": "^3.0.0-beta",
        "netgen/query-translator": "dev-add-ranges-support",
        "oneofftech/k-link-registry-client": "^0.1",
        "php-http/cache-plugin": "^1.4",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/httplug-bundle": "^1.8",
        "pnz/messenger-filesystem-transport-bundle": "0.1.x-dev",
        "ramsey/uuid": "^3.6",
        "ramsey/uuid-doctrine": "^1.4",
        "sensio/framework-extra-bundle": "^5.0",
        "solarium/solarium": "4.0.x-dev as 3.8.1",
        "swagger-api/swagger-ui": "^3.2",
        "symfony/asset": "^4.1",
        "symfony/cache": "^4.1",
        "symfony/console": "^4.1",
        "symfony/dotenv": "^4.1",
        "symfony/event-dispatcher": "^4.1",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^4.1",
        "symfony/monolog-bundle": "^3.1",
        "symfony/proxy-manager-bridge": "^4.1",
        "symfony/security-bundle": "^4.1",
        "symfony/translation": "^4.1",
        "symfony/twig-bundle": "^4.1",
        "symfony/validator": "^4.1",
        "symfony/yaml": "^4.1",
        "zircote/swagger-php": "^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "php-http/mock-client": "^1.0",
        "phpstan/phpstan-shim": "^0.9",
        "phpunit/phpunit": "^7.0",
        "sebastian/comparator": "^3.0",
        "symfony/browser-kit": "^4.1",
        "symfony/web-profiler-bundle": "^4.1"
    },
    "replace": {
        "paragonie/random_compat": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "make cache-warmup": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "extra": {
        "symfony": {
            "id": "01BKVTE0GGJ9MESG49ECBW3DB5",
            "allow-contrib": "true"
        }
    }
}
